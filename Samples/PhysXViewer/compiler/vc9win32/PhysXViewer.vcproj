    <Tool 
      Name="VCLinkerTool" 
      OutputFile="$(OutDir)/.exe" 
      ProgramDatabaseFile="$(OutDir)/.exe.pdb" 
      AdditionalOptions="/NODEFAULTLIB:libcp.lib /NODEFAULTLIB:libcp.lib "
