      AdditionalLibraryDirectories=""
      ImportLibrary="$(OutDir)/$(TargetName).lib"
      GenerateMapFile="TRUE"
      MapFileName="$(OutDir)/.exe.map"
      SubSystem="2"
      GenerateDebugInformation=""
      />
