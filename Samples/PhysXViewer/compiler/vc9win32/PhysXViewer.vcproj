    Name="default|Win32"
    OutputDirectory="$(SolutionDir)/out"
    IntermediateDirectory="Win32/PhysXViewer/default"
    ConfigurationType="1"
    InheritedPropertySheets=""
    CharacterSet="2"
    >
    <Tool
      Name="VCCLCompilerTool"
      AdditionalIncludeDirectories="&quot;../../src&quot;;&quot;../../src/tui&quot;;&quot;../../src/common&quot;;&quot;../../src/globals&quot;;&quot;../../src&quot;;&quot;../../src/tui&quot;;&quot;../../src/common&quot;;&quot;../../src/globals&quot;;"
